      "name": "botStream",
      "triggers": ["botStream", "botstream"],
      "function": "botStream",
      "helptext": "<b>/botStream</b>",
      "description": "This will return all the chat that users have with the Bot, this could be of any type (IM/MIM and ROOMs)",
      "param":"IM or ROOM or ALL",
      "example":"/botStream im or /botStream all",
      "category": "General Bot command",
      "permission": "Bot Admin"
    },
    {
