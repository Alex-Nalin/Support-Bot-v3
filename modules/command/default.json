      "function": "botMessageBlast",
      "helptext": "<b>/botMessageBlast</b>",
      "description": "Sends a message to all IM/MIM or ROOMs or even all Types with a specific message",
      "param":"IM or ROOM or ALL + message",
      "example":"/botMessageBlast IM The bot has been updated",
      "category": "General Bot command",
      "permission": "Bot Admin"
    },
